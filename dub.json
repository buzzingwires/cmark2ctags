{
	"name": "cmark2ctags",
	"description": "Convert MarkDown files to Exuberant Ctags using libcmark.",
	"targetType": "executable",
	"buildTypes": {
		"ldc-production": {
			"dflags": ["-release", "-O2", "-boundscheck=off"]
		},
		"dmd-production": {
			"dflags": ["-release", "-O", "-boundscheck=off", "-L-l:libphobos2.a", "-L-l:libdruntime.a", "-L/usr/lib64/libcmark.a"]
		},
		"ldc-profile": {
			"dflags": ["-g", "-profile", "-release", "-O2", "-boundscheck=off", "-flto=full"]
		},
		"dmd-profile": {
			"dflags": ["-g", "-profile", "-release", "-O", "-boundscheck=off", "-L-l:libphobos2.a", "-L-l:libdruntime.a", "-L/usr/lib64/libcmark.a"]
		},
		"ldc-profile-gc": {
			"dflags": ["-g", "-profile=gc", "-release", "-O2", "-boundscheck=off", "-flto=full"]
		},
		"dmd-profile-gc": {
			"dflags": ["-g", "-profile=gc", "-release", "-O", "-boundscheck=off", "-L-l:libphobos2.a", "-L-l:libdruntime.a", "-L/usr/lib64/libcmark.a"]
		},
		"ldc-coverage": {
			"dflags": ["-g", "-O0", "-static", "-cov"]
		},
		"dmd-coverage": {
			"dflags": ["-g", "-cov"]
		}
	}
}
